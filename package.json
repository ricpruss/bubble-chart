{
    "name": "bubble-chart",
    "version": "2.0.0",
    "description": "Modern bubble chart library using D3.js",
    "type": "module",
    "main": "dist/bubble-chart.js",
    "module": "dist/bubble-chart.esm.js",
    "types": "dist/bubble-chart.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "npm run type-check && rollup -c",
        "dev": "rollup -c -w",
        "prepublishOnly": "npm run build",
        "test:unit": "jest",
        "test:smoke": "node scripts/smoke-test.js",
        "test:integration": "node scripts/test-integration.js",
        "test:integration:browser": "vite --open examples/tests/integration-test.html",
        "test:browser": "vite --open examples/tests/browser-test.html",
        "test:all": "npm run test:unit && npm run test:integration",
        "test:quick": "npm run test:smoke",
        "type-check": "tsc --noEmit",
        "type-check:watch": "tsc --noEmit --watch",
        "demo": "vite --open"
    },
    "keywords": ["d3", "bubble-chart", "visualization", "chart"],
    "author": "Maximiliano Báez González <mxbg.py@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "d3": "^7.8.5"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.1.0",
        "@types/d3": "^7.4.0",
        "@types/jest": "^29.5.0",
        "jest": "^29.0.0",
        "jsdom": "^24.0.0",
        "ts-jest": "^29.1.0",
        "rollup": "^4.0.0",
        "rollup-plugin-dts": "^6.0.0",
        "tslib": "^2.6.0",
        "typescript": "^5.2.0",
        "vite": "^5.2.0"
    },
    "peerDependencies": {
        "d3": "^7.0.0"
    }
}
